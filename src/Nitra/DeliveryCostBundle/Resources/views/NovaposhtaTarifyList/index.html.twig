
{% extends "::base_admin.html.twig" %}

{% block stylesheets %}    {{ parent() }}
    {% endblock %}

{% block javascripts %}    {{ parent() }}
    {% endblock %} 
{% block body %}    {% block page_title %}
        <header>
            <h1>{% trans from "Admin" %}Справочник по ТК "Новая почта"{% endtrans %}</h1>
        </header>
    {% endblock %}
<div class="admin_list main_content">
    <table>
    {% block list_thead %}
        <thead>
            <tr class="list_thead">
                <th class="list_thead_column"></th>
                <th class="list_thead_column"></th>
            </tr>
        </thead>
    {% endblock %}
    {% block list_tbody %}
    <tbody>
        <tr class="list_trow">
        {% block list_row %}
            <td class="td_integer td_id">
                {% block list_td_column_id %} Справочник тарифов по зонам "Склад-склад" {% endblock %}
            </td>
            {% endblock %}
            {% block list_object_actions %}
            <td class="actions">
                <a class="edit " href="{{ path("NitraDeliveryCostBundle_novaposhta_info", { 'type': 'warehouse-warehouse' }) }}"</a>
            </td>
        {% endblock %}
        </tr>
        <tr class="list_trow">
        {% block list_row2 %}
            <td class="td_integer td_id">
                {% block list_td_column_id2 %} Справочник доплат при доставке "Склад-дверь" или "Дверь-склад" {% endblock %}
            </td>
        {% endblock %}
        {% block list_object_actions2 %}
            <td class="actions">
                <a class="edit " href="{{ path("NitraDeliveryCostBundle_novaposhta_info", { 'type': 'warehouse-doors' }) }}"</a>
            </td>
        {% endblock %}
        </tr>
        <tr class="list_trow">
        {% block list_row3 %}
            <td class="td_integer td_id">
                {% block list_td_column_id3 %} Справочник тарифов по зонам "Дверь-дверь" {% endblock %}
            </td>
        {% endblock %}
        {% block list_object_actions3 %}
            <td class="actions">
            <a class="edit " href="{{ path("NitraDeliveryCostBundle_novaposhta_info", { 'type': 'doors-doors' }) }}"</a>
            <script type="text/javascript">
                $(document).ready(function() {
                    $("td.actions a").click(function(e) {
                        e.preventDefault();

                        if ($(this).data('confirm')) {
                            if(!confirm($(this).data('confirm')))
                              return false;
                        }

                        window.location.href = $(this).attr('href');
                    });
                });
            </script>
         </td>
        {% endblock %}
        </tr>
    </tbody>
{% endblock %}

</table>
</div>

  
{% endblock %}
